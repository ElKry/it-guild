						img.img-thumbnail(alt="Благодарственная грамота" src="../img/sochi-blank.jpg")
				.col-xs-6.col-sm-6.col-md-3.col-lg-3
					a.lightbox(title="Благодарственная грамота" href="../img/kaspersky-blank.jpg" target="_blank")
						img.img-thumbnail(alt="Благодарственная грамота" src="../img/kaspersky-blank.jpg")
				.col-xs-6.col-sm-6.col-md-3.col-lg-3
					a.lightbox(title="Благодарственная грамота" href="../img/wincor.jpg" target="_blank")
						img.img-thumbnail(alt="Благодарственная грамота" src="../img/wincor.jpg")